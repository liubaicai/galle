
<%= form_for [@project, @project.publishers.build], url: {action: "create"} do |f| %>
  <%= f.text_field :project_id, class: "hidden" %>
  <div class="form-group">
    <label>标题</label>
    <%= f.text_field :title, type: "text", class: "form-control", placeholder: "" %>
  </div>
  <%= f.submit "确定", class: "btn btn-primary" %>
  <%= link_to '取消', project_path(@project), type: "button", class: "btn btn-default" %>
<% end %>